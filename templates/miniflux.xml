  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1585644478</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>Miniflux is a minimalist and opinionated feed reader.</Description>
